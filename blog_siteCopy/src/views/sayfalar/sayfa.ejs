<script>
    const moduleName = "<%= sayfa[0].data.modulName %>";
    import(`/modules/${moduleName}`).then(module => {
        const config = {
            type: Phaser.AUTO,
            width: <%= gameWidth %>,
            height: <%= gameHeight %>,
                backgroundColor: '<%= backgroundColor %>',
                    scene: {
            preload: preload,
                create: create,
            },
        };
    const game = new Phaser.Game(config);
    function preload() {
        this.load.image('logo', '<%= parts[1].image %>');
    }
    function create() {
        this.add.image(config.width / 2, config.height / 2, 'logo');
    }
        });
</script>